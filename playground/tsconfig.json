{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "jsx": "preserve",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "types": ["@dcloudio/types"],
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
